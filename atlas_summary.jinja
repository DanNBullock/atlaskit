<!doctype html>
<html lang="en">

<head>
<STYLE TYPE="text/css">
BODY {
  font-family    : arial, sans-serif;
}
td, th {
  padding: 10px 0px 0px 20px;
  text-align: left;
}
</STYLE>
</head>

<body>

<h1 style="background-color:#E0E0FF">Probabilistic Atlas Report</h1>

<div>
    <h3>Observer Label Reports</h3>
    <table>
        <tr>
            {% for report in obs_reports %}
                    <td> <a href="{{ report.fname }}">Observer {{ report.obs }} Report</a> </td>
            {% endfor %}
            <td> <a href="inter_report.html">Between Observer Report</a> </td>
        </tr>

    </table>
</div>

<div>
    <h3>Probabilistic Atlas Labels</h3>
    <img src="{{ montage_fname }}">
</div>

</body>

<footer>
    <p>Report generated by atlas_report.py on {{ report_time }}</p>
</footer>

</html>